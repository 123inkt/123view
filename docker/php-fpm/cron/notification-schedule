HOME=/app
0 */1 * * * sudo -u www-data php bin/console mail --frequency=once-per-hour          2>&1
0 */2 * * * sudo -u www-data php bin/console mail --frequency=once-per-two-hours     2>&1
0 */3 * * * sudo -u www-data php bin/console mail --frequency=once-per-three-hours   2>&1
0 */4 * * * sudo -u www-data php bin/console mail --frequency=once-per-four-hours    2>&1
0 0 * * *   sudo -u www-data php bin/console mail --frequency=once-per-day           2>&1
0 0 * * 1   sudo -u www-data php bin/console mail --frequency=once-per-week          2>&1
